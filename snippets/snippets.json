{
  "taroPage": {
    "scope": "javascript,typescript",
    "prefix": "taroPage",
    "body": [
      "import Taro from '@tarojs/taro'",
      "import { View, Button } from '@tarojs/components'",
      "",
      "export default class ${1:Template} extends Taro.Component {",
      "  constructor (props) {",
      "    super(props)",
      "  }",
      "  componentWillMount () {}",
      "  render () {",
      "    return (",
      "      <View>",
      "        <Button>${1:Template}</Button>",
      "      </View>",
      "    )",
      "  }",
      "}"
    ],
    "description": "taro基本结构page"
  },
  "taroLess": {
    "scope": "javascript,typescript",
    "prefix": "taroPageLess",
    "body": [
      "import Taro from '@tarojs/taro'",
      "import { View, Text } from '@tarojs/components'",
      "import styles from './index.module.less'",
      "",
      "export default class ${1:Template} extends Taro.Component {",
      "  constructor (props) {",
      "    super(props)",
      "  }",
      "  componentWillMount () {}",
      "  render () {",
      "    return (",
      "      <View className={styles.${2:container}}>",
      "        <Text>${1:Template}</Text>",
      "      </View>",
      "    )",
      "  }",
      "}"
    ],
    "description": "taro基本结构less版本"
  }
}
